{
  "name": "grpc-host-builder",
  "version": "2.1.1",
  "description": "Lightweight configurator for gRPC host",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:protos": "mkdirp ./tests/generated && pbjs -r greeter -t static-module -w commonjs --no-verify -o ./tests/generated/greeter_pb.js ./tests/protos/greeter.proto",
    "build:types": "mkdirp ./tests/generated && pbts -o ./tests/generated/greeter_pb.d.ts ./tests/generated/greeter_pb.js",
    "build": "npm run build:protos && npm run build:types",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/litichevskiydv/grpc-host-builder.git"
  },
  "keywords": [
    "gRPC",
    "Host",
    "Configurator"
  ],
  "author": "Dmitriy Litichevskiy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/litichevskiydv/grpc-host-builder/issues"
  },
  "homepage": "https://github.com/litichevskiydv/grpc-host-builder#readme",
  "devDependencies": {
    "@grpc/proto-loader": "^0.4.0",
    "coveralls": "^3.0.3",
    "grpc-tools": "^1.7.1",
    "jest": "^24.3.1",
    "jest-extended": "^0.11.1",
    "mkdirp": "^0.5.1",
    "protobufjs": "^6.8.8"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/generated/"
    ]
  },
  "dependencies": {
    "grpc": "^1.20.2",
    "grpc-error": "^1.0.0",
    "processing-context": "^1.0.0"
  }
}
